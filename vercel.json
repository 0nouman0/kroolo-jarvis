{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/auth/(.*)",
      "destination": "/api/auth/$1"
    },
    {
      "source": "/analysis/(.*)",
      "destination": "/api/analysis/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
